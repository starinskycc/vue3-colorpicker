{
  "name": "@starinskycc/vue3-colorpicker",
  "version": "2.3.1",
  "main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "author": "aesoper",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && vite build --config ./vite.config.lib.ts",
    "serve": "vite preview",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install",
    "clean:dist": "rimraf dist",
    "pub": "cd dist && npm publish --registry=https://registry.npmjs.org --access public && cd ..",
    "unpub": "npm unpublish vue3-colorpicker --force --registry=https://registry.npmjs.org",
    "prelink": "cd dist && yarn link && cd .. && yarn link vue3-colorpicker",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "storybook dev",
    "build-storybook": "rimraf storybook-static && storybook build",
    "reinstall": "rimraf node_modules && yarn install"
  },
  "dependencies": {
    "@aesoper/normal-utils": "^0.1.5",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^10.1.2",
    "gradient-parser": "^1.0.2",
    "lodash-es": "^4.17.21",
    "tinycolor2": "^1.4.2",
    "vue": "^3.3.4",
    "vue-types": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@storybook/addon-actions": "^7.6.0",
    "@storybook/addon-docs": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/addon-storysource": "^7.6.0",
    "@storybook/cli": "^7.6.0",
    "@storybook/vue3": "^7.6.0",
    "@storybook/vue3-vite": "^7.6.0",
    "@types/gradient-parser": "^0.1.2",
    "@types/jest": "^29.5.0",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.10.0",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.3.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "babel-loader": "^9.1.3",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.50.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.30",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-embedded": "^1.69.5",
    "storybook": "^7.6.0",
    "stylelint": "^15.10.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.3.0",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.6.0",
    "vue-loader": "^17.3.0",
    "vue-tsc": "^1.8.22"
  }
}